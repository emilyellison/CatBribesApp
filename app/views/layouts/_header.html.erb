    <div class="container">
			<header>
				<h1><%= link_to 'CatBribes', root_url, class: 'brand' %></h1>
				<% if @current_member %>
					<ul class='nav pull-right'>
						<li><%= link_to 'Profile', member_url(@current_member.id) %></li>
